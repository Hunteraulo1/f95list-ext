{
  "name": "f95list-ext",
  "version": "1.6.16",
  "private": false,
  "type": "module",
  "author": {
    "email": "hunteraulo@pm.me",
    "name": "Hunteraulo"
  },
  "license": "UNLICENSED",
  "description": "Extension de F95 France permettant de lister les jeux",
  "repository": {
    "type": "git",
    "url": "https://github.com/Hunteraulo1/f95list-ext"
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    },
    "cz-emoji": {
      "skipQuestions": [
        "scope",
        "body"
      ]
    }
  },
  "scripts": {
    "dev": "vite dev",
    "build": "bun run build:chrome && bun run build:firefox",
    "build:chrome": "BUILD_MODE=chrome vite build",
    "build:firefox": "BUILD_MODE=firefox vite build",
    "check": "bunx @biomejs/biome check --write . && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "lint": "bunx @biomejs/biome lint --write .",
    "format": "bunx @biomejs/biome format --write ."
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@eslint/js": "9.13.0",
    "@sveltejs/kit": "2.7.3",
    "@sveltejs/vite-plugin-svelte": "4.0.0",
    "@types/chrome": "0.0.279",
    "@types/webextension-polyfill": "0.12.1",
    "archiver": "7.0.1",
    "autoprefixer": "10.4.20",
    "bits-ui": "^0.21.16",
    "clsx": "2.1.1",
    "cmdk-sv": "^0.0.18",
    "cross-env": "7.0.3",
    "dotenv": "16.4.5",
    "mode-watcher": "0.4.1",
    "postcss": "8.4.47",
    "postcss-import": "16.1.0",
    "svelte": "5.1.4",
    "svelte-check": "4.0.5",
    "svelte-radix": "^2.0.1",
    "sveltekit-adapter-chrome-extension": "2.0.1",
    "tailwind-merge": "2.5.4",
    "tailwind-variants": "0.2.1",
    "tailwindcss": "3.4.14",
    "ts-node": "10.9.2",
    "tslib": "2.8.0",
    "typescript": "5.6.3",
    "valibot": "0.42.1",
    "vite": "5.4.10",
    "vite-plugin-svelte": "3.0.1",
    "vite-plugin-tailwind-purgecss": "0.3.3"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "@deepkit/type-compiler",
    "@sveltejs/kit",
    "esbuild",
    "svelte-preprocess"
  ]
}
