{
  "name": "f95list-ext",
  "version": "2.2.12",
  "author": {
    "email": "hunteraulo@pm.me",
    "name": "Hunteraulo"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Hunteraulo1/f95list-ext"
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    },
    "cz-emoji": {
      "skipQuestions": [
        "scope",
        "body"
      ]
    }
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "@lucide/svelte": "0.542.0",
    "@tailwindcss/vite": "4.1.12",
    "@tsconfig/svelte": "5.0.5",
    "@types/chrome": "0.1.4",
    "@wxt-dev/module-svelte": "2.0.3",
    "bits-ui": "2.9.5",
    "clsx": "2.1.1",
    "mode-watcher": "1.1.0",
    "paneforge": "1.0.2",
    "svelte": "5.38.6",
    "svelte-check": "4.3.1",
    "svelte-sonner": "1.0.5",
    "tailwind-merge": "3.3.1",
    "tailwind-variants": "3.1.0",
    "tailwindcss": "4.1.12",
    "tslib": "2.8.1",
    "tw-animate-css": "1.3.7",
    "typescript": "5.9.2",
    "valibot": "1.1.0",
    "vaul-svelte": "1.0.0-next.7",
    "wxt": "0.20.8"
  },
  "overrides": {
    "vaul-svelte": ">=1.0.0-next.1"
  },
  "description": "Extension de F95 France permettant de lister les traductions",
  "license": "UNLICENSED",
  "private": false,
  "scripts": {
    "dev:chrome": "wxt --debug",
    "dev:firefox": "wxt -b firefox --debug",
    "build": "bun run build:chrome && bun run build:firefox",
    "build:chrome": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "check": "bunx @biomejs/biome check --write . && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "bunx @biomejs/biome check --write . && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "bunx @biomejs/biome lint --write .",
    "format": "bunx @biomejs/biome format --write .",
    "postinstall": "wxt prepare"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "@tailwindcss/oxide",
    "dtrace-provider",
    "esbuild",
    "spawn-sync"
  ],
  "type": "module",
  "dependencies": {
    "svelte-lazy": "1.2.11"
  }
}
