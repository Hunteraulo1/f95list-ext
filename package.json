{
  "name": "f95list-ext",
  "version": "2.0.7",
  "author": {
    "email": "hunteraulo@pm.me",
    "name": "Hunteraulo"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Hunteraulo1/f95list-ext"
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    },
    "cz-emoji": {
      "skipQuestions": ["scope", "body"]
    }
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@sveltejs/vite-plugin-svelte": "5.0.3",
    "@tsconfig/svelte": "5.0.4",
    "@types/chrome": "0.0.297",
    "@types/webextension-polyfill": "0.12.1",
    "autoprefixer": "10.4.20",
    "bits-ui": "^1.0.0-next.77",
    "clsx": "^2.1.1",
    "cross-env": "7.0.3",
    "mode-watcher": "0.5.0",
    "svelte": "5.18.0",
    "svelte-check": "4.1.4",
    "tailwind-merge": "^2.6.0",
    "tailwind-variants": "^0.3.0",
    "tailwindcss": "3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "tslib": "2.8.1",
    "typescript": "5.7.3",
    "valibot": "1.0.0-beta.12",
    "vite": "6.0.7",
    "vite-plugin-web-extension": "4.4.3",
    "webextension-polyfill": "0.12.0"
  },
  "overrides": {
    "bits-ui": ">=1.0.0-next.1"
  },
  "description": "Extension de F95 France permettant de lister les jeux",
  "license": "UNLICENSED",
  "private": false,
  "scripts": {
    "dev:chrome": "cross-env TARGET=chrome vite dev",
    "dev:firefox": "cross-env TARGET=firefox vite dev",
    "build": "bun run build:chrome && bun run build:firefox",
    "build:chrome": "cross-env TARGET=chrome vite build",
    "build:firefox": "cross-env TARGET=firefox vite build",
    "check": "bunx @biomejs/biome check --write . && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "bunx @biomejs/biome check --write . && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "bunx @biomejs/biome lint --write .",
    "format": "bunx @biomejs/biome format --write ."
  },
  "trustedDependencies": ["@biomejs/biome", "dtrace-provider", "esbuild", "spawn-sync"],
  "type": "module",
  "dependencies": {
    "lucide-svelte": "0.471.0",
    "svelte-lazy": "1.2.11"
  }
}
